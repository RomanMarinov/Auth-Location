<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

        <LinearLayout
            android:id="@+id/ll_frag_auth"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="horizontal">
                    <fragment
                        android:tag="auth"
                        android:id="@+id/frag_auth"
                        android:layout_width="match_parent"
                        tools:layout="@layout/fragment_auth"
                        android:name="com.dev_marinov.wnfxtest2.FragmentAuth"
                        android:layout_height="match_parent">
                    </fragment>

        </LinearLayout>

    <LinearLayout
    android:tag="enter_sms"
    android:id="@+id/ll_frag_enter_sms"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    </LinearLayout>

    <LinearLayout
    android:tag="success"
    android:id="@+id/ll_frag_success"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    </LinearLayout>

    <LinearLayout
        android:tag="finish"
        android:id="@+id/ll_frag_finish"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">

    </LinearLayout>

    <LinearLayout
    android:tag="map"
    android:id="@+id/ll_frag_map"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">
    </LinearLayout>


    <LinearLayout
        android:tag="list_food"
        android:id="@+id/ll_frag_list_food"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">
    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>


